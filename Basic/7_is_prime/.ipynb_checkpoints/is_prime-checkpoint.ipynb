    "# input: a number => output: prime or not prime\n",
    "is_prime = 1\n",
    "number = int ( input())\n",
    "for i in range(2 , number):\n",
    "    if number%i == 0:\n",
    "        is_prime = 0\n",
    "        break\n",
    "if is_prime == 1:\n",
    "    print('prime')\n",
    "else:\n",
    "    print('not prime')"
   ]
